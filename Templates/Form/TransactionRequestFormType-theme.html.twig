    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.autoSendAgreements) }}
        </div>
        <div class="col-md-6">
            {{ form_row(form.operator) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_row(form.registrationCallbackURL) }}
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            {{ form_row(form.successRedirection) }}
        </div>
        <div class="col-md-12">
            {{ form_row(form.cancelRedirection) }}
        </div>
        <div class="col-md-12">
            {{ form_row(form.failRedirection) }}
        </div>
    </div>
